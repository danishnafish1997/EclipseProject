 <!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <context-param>
    <param-name>tempfile.dir</param-name>
    <param-value>tmpfiles</param-value>
  </context-param>
  <listener>
  	<listener-class>com.uploadDownload.FileLocationContextListener</listener-class>
  </listener>
  <servlet>
  	<servlet-name>ReadServlet</servlet-name>
  	<display-name>ReadServlet</display-name>
  	<description></description>
  	<servlet-class>demo.controller.ReadServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ReadExcel</servlet-name>
  	<display-name>ReadExcel</display-name>
  	<description></description>
  	<servlet-class>com.readExcel.ReadExcel</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ExcelToCsv</servlet-name>
  	<display-name>ExcelToCsv</display-name>
  	<description></description>
  	<servlet-class>com.convertToCsv.ExcelToCsv</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Upload</servlet-name>
  	<display-name>Upload</display-name>
  	<description></description>
  	<servlet-class>com.uploadDownload.Upload</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ReadXExcel</servlet-name>
  	<display-name>ReadXExcel</display-name>
  	<description></description>
  	<servlet-class>com.readExcel.ReadXExcel</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ReadServlet</servlet-name>
  	<url-pattern>/ReadServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ReadExcel</servlet-name>
  	<url-pattern>/ReadExcel</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ExcelToCsv</servlet-name>
  	<url-pattern>/ExcelToCsv</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Upload</servlet-name>
  	<url-pattern>/Upload</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ReadXExcel</servlet-name>
  	<url-pattern>/ReadXExcel</url-pattern>
  </servlet-mapping>
  
</web-app>
